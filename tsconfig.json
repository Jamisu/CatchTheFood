{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "dom",
      "es2017"
    ],
    /* Modules */
    "module": "commonjs",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
